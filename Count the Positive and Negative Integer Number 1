import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int poscount=0;
        int negcount=0;
        for(int i=0;i<n;i++)
        {
            int a=sc.nextInt();
            if(a>0)
                poscount++;
            else
                negcount++;
        }
        System.out.printf("Count of Positive Integer is %.2f\n",(double)poscount);
          System.out.printf("Count of Negative Integer is %.2f\n",(double)negcount);
       
           
    }
}
