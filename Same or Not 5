import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
int n = sc.nextInt();
boolean hasEven = false;
boolean hasOdd = false;
for (int i = 0; i < n; i++) {
  int num = sc.nextInt();
  if (num % 2 == 0) {

     hasEven = true;

} else 
  {
 hasOdd = true;
 }
 }
 if (hasEven && hasOdd)
 {
System.out.println("Mixed");

 } else if (hasEven)
 {

 System.out.println("Even");

 } else {

System.out.println("Odd");
 }
}
}

    
